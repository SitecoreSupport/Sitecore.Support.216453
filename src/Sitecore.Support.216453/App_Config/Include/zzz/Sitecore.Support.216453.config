<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <renderField>
        <processor type="Sitecore.Support.Pipelines.RenderField.GetImageFieldValue, Sitecore.Support.216453"
                   patch:instead="*[@type='Sitecore.Pipelines.RenderField.GetImageFieldValue, Sitecore.Kernel']" />
        <processor type="Sitecore.Support.Pipelines.RenderField.ProtectedImageLinkRenderer, Sitecore.Support.216453"
                   patch:instead="*[@type='Sitecore.Pipelines.RenderField.ProtectedImageLinkRenderer, Sitecore.Kernel']" />
      </renderField>
    </pipelines>
  </sitecore>
</configuration>